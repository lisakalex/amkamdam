    margin: auto;
    position: absolute;
    top: 50%;
    left: 50%;
    width: 100%;
    height: 100%;
    transform: translate(-50%, -50%);
}
.aside-news__inner {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    gap: 8px;
}

.aside-news__title {
    display: block;
    font-family: Noto Sans;
    font-size: 12px;
    font-weight: 600;
    line-height: 16.2px;
    text-align: left;
    color: #211B2E;
    width: 100%;
    margin: auto 0;
}
.aside-news__info {
    display: flex;
    flex-wrap: nowrap;
    width: 100%;
    gap: 4px;
    margin-top: auto;
    align-items: center;
