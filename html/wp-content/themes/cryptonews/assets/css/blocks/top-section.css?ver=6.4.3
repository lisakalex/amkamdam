.homepage-block__top-section {
    display: block;
    width: 100%;
    padding-bottom: 32px;
    background: #FFFFFF;
}
.homepage-block__top-section.pt-32 {
    padding-top: 32px;
}
.homepage-block__top-section-grid {
    display: grid;
    gap: 24px 30px;
    grid-template-columns: 2fr 1fr 1fr;
    width: 100%;
}
@media screen and (max-width: 1359px) {
    .homepage-block__top-section-grid {
        grid-template-columns: 1fr 1fr;
    }
    .homepage-block__top-section-column-main {
        grid-column: 1 / -1;
    }
}
.homepage-block__top-section-title {
    margin: 0;
    height: 32px;
}

.homepage-block__top-section-column-main {
    display: grid;
    gap: 16px;
    grid-template-rows: 32px 2fr auto;
}

.homepage-block__top-section-column-secondary {
    display: grid;
    gap: 16px;
    grid-template-rows: 32px auto;
}
.homepage-block__top-section-column-secondary-data {
    display: grid;
    gap: 16px;
    grid-template-rows: repeat(3, 1fr);
}
@media screen and (max-width: 767px) {
    .homepage-block__top-section-column-secondary-data {
        display: block;
        width: 510px;
        position: relative;
    }

    .homepage-block__top-section-column-secondary-data .slick-track {
        display: flex;
    }
    .homepage-block__top-section-column-secondary-data .slick-track .slick-slide {
        height: auto;
    }

    .slider_homepage-block__top-section-column-secondary-data .story-one-cell {
        aspect-ratio: 3 / 2;
    }
    .slider_homepage-block__top-section-column-secondary-data .slick-list {
        padding: 0 50px 0 0 !important;
        overflow: visible;
    }
    .slider_homepage-block__top-section-column-secondary-data .slick-slide {
        padding-right: 16px;
    }
}
@media screen and (max-width: 575px) {
    .homepage-block__top-section-column-secondary-data {
        width: calc(100vw - 30px);
    }
}
.homepage-block__top-section-column-main-bottom {
    display: flex;
    flex-wrap: nowrap;
    gap: 16px 30px;
}

.homepage-block__top-section-column-secondary-data .slick-slide,
.homepage-block__top-section-column-main-bottom .slick-slide {
    transition: all .3s ease;
}
.homepage-block__top-section-column-secondary-data .slick-slide:not(.slick-current),
.homepage-block__top-section-column-main-bottom .slick-slide:not(.slick-current) {
    filter: blur(2px);
    opacity: 0.54;
}
.slick-dots {
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 16px 0 0;
    gap: 8px;
}
.slick-dots li {
    margin: 0;
    width: 8px;
    height: 8px;
}
.slick-dots li:after,
.slick-dots li:before {
    display: none;
}
.slick-dots li button {
    border-radius: 0;
    width: 100%;
    height: 100%;
    padding: 0;
    background: #E5C0DA;
    margin: 0;
    position: absolute;
    left: 0;
    top: 0;
}
.slick-dots li.slick-active button {
    background: #591C7E;
}

@media screen and (max-width: 767px) {
    .homepage-block__top-section-grid {
        grid-template-rows: auto auto auto;
        grid-template-columns: 1fr;
    }

    .homepage-block__top-section-column-main {
        grid-column: 1;
    }

    .homepage-block__top-section-column-main-bottom {
        display: block;
        width: 510px;
        position: relative;
    }
    .homepage-block__top-section-column-main-bottom .slick-track {
        display: flex;
    }
   .homepage-block__top-section-column-main-bottom .slick-track .slick-slide {
       height: auto;
    }

    .slider_homepage-block__top-section-column-main-bottom .slick-list {
        padding: 0 50px 0 0 !important;
        overflow: visible;
    }
    .slider_homepage-block__top-section-column-main-bottom .slick-slide {
        padding-right: 16px;
    }
}
@media screen and (max-width: 575px) {
    .homepage-block__top-section-column-main-bottom {
        width: calc(100vw - 30px);
    }
    .homepage-block__top-section-grid {
        grid-template-columns: calc(100vw - 30px)
    }
}
.homepage-block__top-section-column-secondary-data .story-one-cell__wrap,
.homepage-block__top-section-column-secondary-data .story-one-video__wrap,
.homepage-block__top-section-column-secondary-data .story-one-video__wrap .story-one-video {
    height: 100%;
}
.story-one-cell__wrap {
    display: block;
    width: 100%;
    height: 200px;
}
.story-one-cell {
    display: flex;
    align-items: flex-end;
    justify-content: flex-end;
    width: 100%;
    height: 100%;
    position: relative;
    padding: 50px 8px 8px 36px;
}
a.story-one-cell:hover {
    text-decoration: none;
}
a.story-one-cell:hover .story-one-cell__title {
    text-decoration: underline;
}
.story-one-cell__bg {
    display: block;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background-color: #F3F3F3;
    background-position: 50%;
    background-repeat: no-repeat;
    background-size: cover;
}
.story-one-cell__label {
    display: block;
    position: absolute;
    top: 12px;
    right: 12px;
    font-family: Noto Sans;
    font-size: 12px;
    font-weight: 600;
    line-height: 16.2px;
    text-align: left;
    color: #fff;
    padding: 4px;
    background: #A72E83;
}
.story-one-cell__inner {
    width: 100%;
    background: #ffffff;
    padding: 8px;
    position: relative;
}
.story-one-cell__inner-style-left {
    position: absolute;
    bottom: 4px;
    left: -4px;
    width: 4px;
    top: 0;
    background: #DC5992;
}
.story-one-cell__inner-style-top:after,
.story-one-cell__inner-style-left:before,
.story-one-cell__inner-style-left:after {
    content: '';
    display: block;
    position: absolute;
    width: 0;
    height: 0;
    border: 2px solid transparent;
}
.story-one-cell__inner-style-left:before {
    top: 100%;
    left: 0;
    border-top-color: #DC5992;
    border-right-color: #DC5992;
}
.story-one-cell__inner-style-left:after {
    bottom: 100%;
    left: 0;
    border-bottom-color: #DC5992;
    border-right-color: #DC5992;
}
.story-one-cell__inner-style-top {
    position: absolute;
    left: 0;
    top: -4px;
    height: 4px;
    right: 4px;
    background: #A72E83;
}
.story-one-cell__inner-style-top:after {
    left: 100%;
    top: 0;
    border-bottom-color: #A72E83;
    border-left-color: #A72E83;
}
.story-one-cell__title {
    display: block;
    font-family: Noto Sans;
    font-size: 14px;
    font-weight: 700;
    line-height: 20px;
    text-align: left;
    color: #211B2E;
    margin: 0 0 4px;
}
.story-one-cell__info {
    display: flex;
    flex-wrap: nowrap;
    justify-content: space-between;
    align-items: center;
}
.story-one-cell__author {
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
    gap: 4px;
    font-family: Noto Sans;
    font-size: 10px;
    font-weight: 400;
    line-height: 10px;
    text-align: left;
    color: #737373;
}
.story-one-cell__author-ava {
    display: flex;
    width: 16px;
    height: 16px;
    min-width: 16px;
    align-items: center;
    justify-content: center;
    overflow: hidden;
    border-radius: 50%;
    object-fit: cover;

}
.story-one-cell__author-ava img {
    display: block;
    max-height: 100%;
    max-height: 100%;
    object-fit: cover;
}
.story-one-cell__time {
    font-family: Noto Sans;
    font-size: 10px;
    font-weight: 400;
    line-height: 16px;
    text-align: right;
    color: #737373;
    white-space: nowrap;
}

.story-one-cell--lg {
    padding: 48px 24px 24px 110px;
}
.story-one-cell--lg .story-one-cell__inner {
    padding: 16px 24px;
    background: #fff;
}
.story-one-cell--lg .story-one-cell__title {
    font-size: 24px;
    font-weight: 700;
    line-height: 32.4px;
    margin-bottom: 8px;
}
.story-one-cell--lg .story-one-cell__time {
    margin-left: auto;
}
.story-one-cell--lg .story-one-cell__time,
.story-one-cell--lg .story-one-cell__author {
    font-size: 12px;
    line-height: 12px;
}
.story-one-cell--lg .story-one-cell__inner-style-left {
    bottom: 24px;
    left: -24px;
    width: 24px;
}
.story-one-cell--lg .story-one-cell__inner-style-top:after,
.story-one-cell--lg .story-one-cell__inner-style-left:before,
.story-one-cell--lg .story-one-cell__inner-style-left:after {
    border-width: 12px;
}
.story-one-cell--lg .story-one-cell__inner-style-top {
    top: -24px;
    height: 24px;
    right: 24px;
}

@media screen and (max-width: 1359px) {
    .story-one-cell--lg {
        aspect-ratio: 13 / 8;
    }
}
@media screen and (max-width: 767px) {
    .story-one-cell--lg {
        padding: 32px 16px 16px 76px;
    }
    .story-one-cell--lg .story-one-cell__inner {
        padding: 8px 8px 8px 16px;
    }
    .story-one-cell--lg .story-one-cell__title {
        font-size: 18px;
        line-height: 24px;
        margin-bottom: 4px;
    }
    .story-one-cell--lg .story-one-cell__time,
    .story-one-cell--lg .story-one-cell__author {
        font-size: 10px;
        line-height: 12px;
    }
    .story-one-cell--lg .story-one-cell__inner-style-left {
        bottom: 12px;
        left: -12px;
        width: 12px;
    }
    .story-one-cell--lg .story-one-cell__inner-style-top:after,
    .story-one-cell--lg .story-one-cell__inner-style-left:before,
    .story-one-cell--lg .story-one-cell__inner-style-left:after {
        border-width: 6px;
    }
    .story-one-cell--lg .story-one-cell__inner-style-top {
        top: -12px;
        height: 12px;
        right: 12px;
    }
}

@media screen and (max-width: 575px) {
    .story-one-cell--lg {
        width: 100vw;
        margin-left: -15px;
    }
}
.story-one-video__wrap {
    display: block;
    width: 100%;
}
.story-one-video {
    display: flex;
    width: 100%;
    position: relative;
}
a.story-one-video:hover {
    text-decoration: none;
}
a.story-one-video:hover .story-one-video__title {
    text-decoration: underline;
}
@media screen and (max-width: 1359px) {
    .story-one-video {
        aspect-ratio: 3 / 2;
    }
}
.story-one-video iframe,
.story-one-video video {
    display: block;
    width: 100%;
    height: 100%;
}
.story-one-video {
    display: flex;
    align-items: flex-end;
    justify-content: flex-end;
    width: 100%;
    min-height: 200px;
    position: relative;
    padding: 16px;
}
.story-one-video__bg {
    display: block;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background-position: 50%;
    background-repeat: no-repeat;
    background-size: cover;
}
.story-one-video__bg:after {
    content: '';
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    background: linear-gradient(180deg, rgba(33, 27, 46, 0) 39.38%, rgba(33, 27, 46, 0.8) 55.68%, #211B2E 82.73%);
}
.story-one-video__icon {
    display: block;
    position: absolute;
    top: 50%;
    left: 50%;
    width: 60px;
    height: 40px;
    content: url(../../../images/youtube-button.svg);
    transform: translate(-50%, -50%);
}
.story-one-video__inner {
    width: 100%;
    padding: 0;
    position: relative;
}
.story-one-video__title {
    display: block;
    font-family: Noto Sans;
    font-size: 16px;
    font-weight: 700;
    line-height: 24px;
    text-align: left;
    text-shadow: 0px 0px 4px #211B2E;
    color: #fff;
    margin: 0;
}

.aside-news-list {
    display: grid;
    height: 100%;
    background: #F3F3F3;
    padding: 16px 16px 0;
    grid-template-rows: auto 1fr 48px;
}

@media screen and (max-width: 767px) {
    .aside-news-list {
        margin: -16px;
        grid-template-rows: auto 1fr 100px;
    }
}
.aside-news-list__title {
    display: block;
    font-family: Noto Sans;
    font-size: 18px;
    font-weight: 700;
    line-height: 24px;
    text-align: left;
    color: #211B2E;
    margin: 0;
    padding: 0 0 16px;
}
.aside-news-list__control {
    display: flex;
    width: calc(100% + 32px);
    margin-left: -16px;
    height: 48px;
    align-items: center;
    justify-content: center;
    font-family: Noto Sans;
    font-size: 14px;
    font-weight: 700;
    line-height: 20px;
    text-align: center;
    color: #A72E83;
    gap: 8px;
    border: 1px solid #E5C0DA;
    background-color: #ffffff;
    text-align: center;
}
@media screen and (max-width: 767px) {
    .aside-news-list__control {
        width: 100%;
        margin-left: 0px;
    }
}
.aside-news-list__control-icon {
    content: url(../../../images/arrow-news-right.svg);
}
.aside-news-list__control:hover {
    text-decoration: none;
    color: #A72E83;
}

.aside-news {
