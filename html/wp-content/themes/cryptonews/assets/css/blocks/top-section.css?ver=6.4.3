.aside-news__author {
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
    gap: 4px;
    font-family: Noto Sans;
    font-size: 10px;
    font-weight: 400;
    line-height: 10px;
    text-align: left;
    color: #211B2E;
}
.aside-news__author-ava {
    display: flex;
    width: 16px;
    height: 16px;
    min-width: 16px;
    align-items: center;
    justify-content: center;
    overflow: hidden;
    border-radius: 50%;
    object-fit: cover;

}
.aside-news__author-ava img {
    display: block;
    max-height: 100%;
    max-height: 100%;
    object-fit: cover;
}
.aside-news__time {
    font-family: Noto Sans;
    font-size: 10px;
    font-weight: 400;
    line-height: 16px;
    text-align: right;
    color: #737373;
    white-space: nowrap;
}

/* section-head */
.section-head {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    gap: 15px;
    align-items: center;
    margin: 0;
}
.section-head__title {
    display: flex;
    flex-wrap: nowrap;
    align-items: flex-start;
    font-family: Noto Sans;
    font-size: 24px;
    font-weight: 700;
    line-height: 32.4px;
    gap: 10px;
    margin: 0 0 16px;
}
.section-head__title .title-icon {
    display: flex;
    width: 32px;
    height: 32px;
    align-items: center;
    justify-content: center;
}
.section-head__title .title-icon svg,
.section-head__title .title-icon img {
    display: block;
    max-width: 100%;
    max-height: 100%;
}
.section-head__controls {
    display: flex;
    margin-left: auto;
    gap: 8px;
    align-items: center;
    min-height: 32px;
    margin-bottom: 16px;
}
.section-head__controls-link {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 20px;
    height: 20px;
    min-width: 20px;
    padding: 5px;
    content: url(../../../images/pagination-arrow.svg);
    border: 1px solid #E5C0DA;
}
.section-head__controls-link.white {
    content: url(../../../images/pagination-arrow-white.svg);
    border: none;
    background: #A72E83;
}
.section-head__controls-link.prev {
    transform: rotate(180deg);
}
.section-head__controls-link.slick-arrow:hover {
    background: none;
}
.section-head__controls-link.white.slick-arrow:hover {
    background: #A72E83;
}
.section-head__controls-link.slick-disabled {
    opacity: 0.5;
    cursor: default;
}
.section-head__controls-more {
    font-family: Noto Sans;
    font-size: 14px;
    font-weight: 700;
    line-height: 20px;
    text-align: center;
    color: #A72E83;
}
.section-head__controls-more:after {
    content: url(../../../images/pagination-arrow.svg);
    margin-left: 8px;
}
.section-head__controls-more:hover {
    color: #A72E83;
    text-decoration: none;
}